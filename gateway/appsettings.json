{
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "users",
        "ClusterId": "users",
        "Match": { "Path": "/users/{**catch-all}" }
      },
      {
        "RouteId": "novels",
        "ClusterId": "novels",
        "Match": { "Path": "/novels/{**catch-all}" }
      }
    ],
    "Clusters": {
      "users": { "Destinations": { "d1": { "Address": "http://users:5000/" } } },
      "novels": { "Destinations": { "d1": { "Address": "http://novels:5000/" } } }
    }
  }
}
